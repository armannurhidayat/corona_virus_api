<odoo>
	<data>
		
		<record id="corona_virus_squence" model="ir.sequence">
			<field name="name">Corona Virus Squence</field>
			<field name="code">corona.virus</field>
			<field name="prefix">Virus/%(day)s%(month)s%(y)s/</field>
			<field name="suffix"></field>
			<field name="padding">3</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
		</record>

	</data>
</odoo>